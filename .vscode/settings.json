    "diffEditor.insertedTextBackground": "#A7C79522",
    "diffEditor.removedTextBackground": "#E5737322",
    "editorGutter.addedBackground": "#A7C795",
    "editorGutter.modifiedBackground": "#8FB3D9",
    "editorGutter.deletedBackground": "#E57373",

    "activityBar.background": "#000000",
    "activityBar.foreground": "#E5E9F0",
    "sideBar.background": "#000000",
    "sideBar.foreground": "#E5E9F0",

    "editorGroupHeader.tabsBackground": "#000000",
    "tab.activeBorderTop": "#8FB3D9",
    "panel.background": "#000000",
    "panel.border": "#111111",

    "statusBar.background": "#000000",
    "statusBar.foreground": "#E5E9F0",
    "titleBar.activeBackground": "#000000",
    "titleBar.activeForeground": "#E5E9F0",

    "terminal.background": "#000000",
    "terminal.foreground": "#E5E9F0",
    "terminalCursor.foreground": "#E5E9F0",
    "terminal.selectionBackground": "#2A3140",

    "terminal.ansiBlack": "#000000",
    "terminal.ansiRed": "#E57373",
    "terminal.ansiGreen": "#A7C795",
    "terminal.ansiYellow": "#F1D8A5",
    "terminal.ansiBlue": "#8FB3D9",
    "terminal.ansiMagenta": "#C895BF",
    "terminal.ansiCyan": "#8ED1E3",
    "terminal.ansiWhite": "#E5E9F0",
    "terminal.ansiBrightBlack": "#555A67",
    "terminal.ansiBrightRed": "#EF9A9A",
    "terminal.ansiBrightGreen": "#B5D6A4",
    "terminal.ansiBrightYellow": "#F5E0B5",
    "terminal.ansiBrightBlue": "#9CC7EA",
    "terminal.ansiBrightMagenta": "#D8A5D2",
    "terminal.ansiBrightCyan": "#9FE3F2",
    "terminal.ansiBrightWhite": "#F2F4F8"
