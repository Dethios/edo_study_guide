    "foreground": "#E5E9F0",
    "focusBorder": "#8FB3D9",
    "selection.background": "#2A3140",

    "editor.background": "#000000",
    "editor.foreground": "#E5E9F0",
    "editor.lineHighlightBackground": "#0D0D0D",
    "editor.selectionBackground": "#2A3140",
    "editor.selectionHighlightBackground": "#2A314080",
    "editor.inactiveSelectionBackground": "#2A314040",
