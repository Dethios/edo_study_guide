  // ===================================================================
  // CHAT / ASSISTANTS
  // ===================================================================
  "chat.editor.wordWrap": "on",

  // ===================================================================
  // CHATGPT
  // ===================================================================
  "chatgpt.runCodexInWindowsSubsystemForLinux": true,

  // ===================================================================
  // CSS
  // ===================================================================
  "css.format.braceStyle": "expand",

  // ===================================================================
  // EDITOR
  // ===================================================================
  "editor.aiStats.enabled": true,
  "editor.allowVariableFontsInAccessibilityMode": true,
  "editor.autoIndentOnPaste": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.codeActions.triggerOnFocusChange": true,
  "editor.codeActionsOnSave": {},
  "editor.codeLensFontFamily": "Fira Code",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.detectIndentation": false,
  "editor.fontFamily": "'Fira Code', Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 12,
  "editor.fontVariations": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "modificationsIfAvailable",
  "editor.formatOnType": true,
  "editor.indentSize": "tabSize",
  "editor.inlineSuggest.enabled": true,
  "editor.inlineSuggest.fontFamily": "'Fira Code'",
  "editor.inlineSuggest.showToolbar": "onHover",
  "editor.insertSpaces": true,
  "editor.linkedEditing": true,
  "editor.maxTokenizationLineLength": 200000,
  "editor.minimap.enabled": false,
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "all",
  "editor.smoothScrolling": true,
  "editor.stickyTabStops": true,
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.trimWhitespaceOnDelete": true,
  "editor.wordBasedSuggestions": "matchingDocuments",
  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 100,
  "editor.wrapOnEscapedLineFeeds": true,
  "editor.wrappingIndent": "indent",

  // ===================================================================
  // TOML
  // ===================================================================
  "evenBetterToml.formatter.alignComments": true,
  "evenBetterToml.formatter.alignEntries": true,
  "evenBetterToml.formatter.allowedBlankLines": 1,
  "evenBetterToml.formatter.arrayAutoCollapse": true,
  "evenBetterToml.formatter.arrayAutoExpand": true,
  "evenBetterToml.formatter.arrayTrailingComma": true,
  "evenBetterToml.formatter.columnWidth": 100,
  "evenBetterToml.formatter.indentEntries": true,
  "evenBetterToml.formatter.indentTables": true,
  "evenBetterToml.formatter.reorderArrays": true,
  "evenBetterToml.formatter.reorderInlineTables": true,
  "evenBetterToml.formatter.reorderKeys": true,
  "evenBetterToml.formatter.trailingNewline": true,
  "evenBetterToml.schema.links": true,
  "evenBetterToml.semanticTokens": true,

  // ===================================================================
  // EXPLORER
  // ===================================================================
  "explorer.confirmDragAndDrop": false,

  // ===================================================================
  // EXTENSIONS
  // ===================================================================
  "extensions.autoCheckUpdates": true,
  "extensions.autoUpdate": true,

  // ===================================================================
  // FILES
  // ===================================================================
  "files.autoGuessEncoding": true,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 1000,
  "files.autoSaveWhenNoErrors": true,
  "files.eol": "\n",
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.git/**": true,
    "**/.texlive202*/**": true,
    "**/build/**": true,
    "**/node_modules/**": true,
    "**/out/**": true
  },

  // ===================================================================
  // GIT
  // ===================================================================
  "git.allowNoVerifyCommit": true,
  "git.autoStash": true,
  "git.autofetch": true,
  "git.autofetchPeriod": 180,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.mergeEditor": true,
  "git.postCommitCommand": "sync",
  "git.pruneOnFetch": true,
  "git.replaceTagsWhenPull": true,
  "git.repositoryScanMaxDepth": 2,
  "git.useCommitInputAsStashMessage": true,
  "git.useEditorAsCommitInput": false,
  "git.useEditorAsCommittingEditor": false,

  // ===================================================================
  // GITLENS
  // ===================================================================
  "gitlens.ai.model": "vscode",
  "gitlens.ai.vscode.model": "copilot:gpt-4.1",

  // ===================================================================
  // HTML
  // ===================================================================
  "html.format.indentHandlebars": true,
  "html.format.indentInnerHtml": true,
  "html.format.templating": true,
  "html.format.wrapLineLength": 100,

  // ===================================================================
  // JAVASCRIPT
  // ===================================================================
  "javascript.referencesCodeLens.enabled": true,
  "javascript.referencesCodeLens.showOnAllFunctions": true,

  // ===================================================================
  // JSON
  // ===================================================================
  "json.schemaDownload.enable": false,

  // ===================================================================
  // LATEX UTILITIES
  // ===================================================================
  "latex-utilities.formattedPaste.maxLineLength": 100,
  "latex-utilities.formattedPaste.useAsDefault": true,
  "latex-utilities.liveReformat.enabled": true,
  "latex-utilities.liveReformat.languages": ["tex", "doctex", "latex", "rsweave", "jlweave"],
  "latex-utilities.liveReformat.snippets": [
    {
      "body": "$1_$2",
      "description": "auto subscript",
      "mode": "maths",
      "prefix": "([A-Za-z}\\)\\]])(\\d)$",
      "triggerWhenComplete": true
    },
    {
      "body": "$1_{$2}",
      "description": "auto escape subscript",
      "mode": "maths",
      "prefix": "([A-Za-z}\\)\\]]) ?_(\\d\\d)$",
      "triggerWhenComplete": true
    },
    {
      "body": "$1$2",
      "description": "remove extraneous sub/superscript space",
      "mode": "maths",
      "prefix": "(\\S) ([\\^_])$",
      "priority": 2,
      "triggerWhenComplete": true
    },
    {
      "body": "$1^{$2}",
      "description": "auto escape superscript",
      "mode": "maths",
      "prefix": "([A-Za-z}\\)\\]]) ?\\^ ?(\\d\\d|[\\+\\-] ?(?:\\d|[A-Za-z]|\\\\\\w+))$",
      "priority": 2,
      "triggerWhenComplete": true
    },
    {
      "body": "$1 $2",
      "description": "whitespace before operators",
      "mode": "maths",
      "prefix": "([^ \u0026\\\\\\+\\-=\u003c\u003e\\|!~@])([\\+\\-=\u003c\u003e])$",
      "priority": -1,
      "triggerWhenComplete": true
    },
    {
      "body": "$1 $2",
      "description": "whitespace after operators",
      "mode": "maths",
      "prefix": "([\\+\\-=\u003c\u003e])([^ \u0026\\\\\\+\\-=\u003c\u003e\\|!~])$",
      "priority": -1,
      "triggerWhenComplete": true
    },
    {
      "body": "\\dots ",
      "description": "⋯",
      "mode": "maths",
      "prefix": "\\.\\.\\.$",
      "triggerWhenComplete": true
    },
    {
      "body": "\\implies ",
      "description": "⇒",
      "mode": "maths",
      "prefix": "=\u003e$",
      "triggerWhenComplete": true
    },
    {
      "body": "\\impliedby ",
      "description": "implied by",
      "mode": "maths",
      "prefix": "=\u003c$",
      "triggerWhenComplete": true
    },
    {
      "body": "\\frac{$1}{$2} ",
      "description": "fraction (empty)",
      "mode": "maths",
      "prefix": "//$",
      "triggerWhenComplete": true
    },
    {
      "body": "\\frac{$1}{$1}$0",
      "description": "fraction (from regex)",
      "mode": "maths",
      "prefix": "(([\\d\\.]+)|([\\d\\.]*)(\\\\)?([A-Za-z]+)((\\^|_)(\\{\\d+\\}|\\d|[A-Za-z]|\\\\\\w+))*!?)\\/$",
      "triggerWhenComplete": true
    },
    {
      "body": "SPECIAL_ACTION_FRACTION",
      "description": "fraction (parsed)",
      "mode": "maths",
      "noPlaceholders": false,
      "prefix": "([\\)\\]}]) ?/$",
      "triggerWhenComplete": true
    },
    {
      "body": "sympy $1 sympy",
      "description": "sympy block",
      "mode": "maths",
      "prefix": "sympy$",
      "triggerWhenComplete": false
    },
    {
      "body": "SPECIAL_ACTION_BREAK",
      "mode": "maths",
      "prefix": "sympy.+$",
      "priority": 2,
      "triggerWhenComplete": true
    },
    {
      "body": "SPECIAL_ACTION_SYMPY",
      "description": "sympy",
      "mode": "maths",
      "prefix": "sympy ?(.+?) ?sympy ?$",
      "priority": 3,
      "triggerWhenComplete": true
    },
    {
      "body": "$1\\iff ",
      "description": "⇔",
      "mode": "maths",
      "prefix": "(^|[^\\\\])\\biff$",
      "triggerWhenComplete": true
    },
    {
      "body": "$1\\in ",
      "description": "in",
      "mode": "maths",
      "prefix": "(^|[^\\\\])\\binn$",
      "triggerWhenComplete": true
    },
    {
      "body": "$1\\not\\in ",
      "description": "∈",
      "mode": "maths",
      "prefix": "(^|[^\\\\])\\bnotin$",
      "triggerWhenComplete": true
    },
    {
      "body": " \\neq ",
      "description": "neq",
      "mode": "maths",
      "prefix": " ?!=$",
      "triggerWhenComplete": true
    },
    {
      "body": "\u0026= ",
      "description": "aligned equal",
      "mode": "maths",
      "prefix": "==$",
      "priority": 1,
      "triggerWhenComplete": true
    },
    {
      "body": " \\approx ",
      "description": "≈",
      "mode": "maths",
      "prefix": " ?~=$",
      "triggerWhenComplete": true
    },
    {
      "body": " \\sim ",
      "description": "∼",
      "mode": "maths",
      "prefix": " ?~~$",
      "triggerWhenComplete": true
    },
    {
      "body": " \\geq ",
      "description": "≥",
      "mode": "maths",
      "prefix": " ?\u003e=$",
      "triggerWhenComplete": true
    },
    {
      "body": " \\leq ",
      "description": "≤",
      "mode": "maths",
      "prefix": " ?\u003c=$",
      "triggerWhenComplete": true
    },
    {
      "body": " \\gg ",
      "description": "≫",
      "mode": "maths",
      "prefix": " ?\u003e\u003e$",
      "triggerWhenComplete": true
    },
    {
      "body": " \\ll ",
      "description": "≪",
      "mode": "maths",
      "prefix": " ?\u003c\u003c$",
      "triggerWhenComplete": true
    },
    {
      "body": " \\times ",
      "description": "×",
      "mode": "maths",
      "prefix": " ?xx$",
      "triggerWhenComplete": true
    },
    {
      "body": " \\cdot ",
      "description": "⋅",
      "mode": "maths",
      "prefix": " ?\\*\\*$",
      "triggerWhenComplete": true
    },
    {
      "body": "$1\\to ",
      "description": "→",
      "mode": "maths",
      "prefix": "(^|[^\\\\]\\b|[ ,\\)\\]\\}]\\w*)(to|-\u003e)$",
      "triggerWhenComplete": true
    },
    {
      "body": " \\mapsto ",
      "description": "↦",
      "mode": "maths",
      "prefix": " ?(?:\\|-\u003e|!\u003e)$",
      "priority": 1.1,
      "triggerWhenComplete": true
    },
    {
      "body": "$1\\arc$2 ",
      "description": "arc(trig)",
      "mode": "maths",
      "prefix": "(^|[^\\\\])a(?:rc)?(sin|cos|tan|cot|csc|sec)$",
      "triggerWhenComplete": true
    },
    {
      "body": "$1\\$2 ",
      "description": "un-backslashed operator",
      "mode": "maths",
      "prefix": "(^|[^\\\\])(sin|cos|tan|cot|csc|sec|min|max|log|exp)$",
      "triggerWhenComplete": true
    },
    {
      "body": "$1\\$2",
      "description": "pi",
      "mode": "maths",
      "prefix": "(^|[^\\\\])(pi)$",
      "triggerWhenComplete": true
    },
    {
      "body": "\\vec{$1}",
      "description": "vector",
      "mode": "maths",
      "prefix": "((?:\\b|\\\\)\\w{1,7})(,\\.|\\.,)$",
      "triggerWhenComplete": true
    },
    {
      "body": "\\tilde{$1}",
      "description": "tilde",
      "mode": "maths",
      "prefix": "(\\\\?[\\w\\^]{1,7})~ $",
      "triggerWhenComplete": true
    },
    {
      "body": "\\dot{$1}",
      "description": "dot",
      "mode": "maths",
      "prefix": "(\\\\?[\\w\\^]{1,7})\\. $",
      "triggerWhenComplete": true
    },
    {
      "body": "\\ddot{$1}",
      "description": "ddot",
      "mode": "maths",
      "prefix": "(\\\\?[\\w\\^]{1,7})\\.\\. $",
      "triggerWhenComplete": true
    },
    {
      "body": "\\overline{$1}",
      "description": "overline",
      "mode": "maths",
      "prefix": "\\bbar$",
      "triggerWhenComplete": true
    },
    {
      "body": "\\overline{$1}",
      "description": "overline",
      "mode": "maths",
      "prefix": "\\b(\\\\?[\\w\\^{}]{1,3})bar$",
      "triggerWhenComplete": true
    },
    {
      "body": "$1\\hat{$1}",
      "description": "hat",
      "mode": "maths",
      "prefix": "(^|[^\\\\])\\bhat$",
      "triggerWhenComplete": true
    },
    {
      "body": "\\hat{$1}",
      "description": "hat",
      "mode": "maths",
      "prefix": "\\b([\\w\\^{}])hat$",
      "triggerWhenComplete": true
    },
    {
      "body": "\\) $1",
      "description": "space after inline maths",
      "mode": "any",
      "prefix": "\\\\\\)(\\w)$",
      "triggerWhenComplete": true
    },
    {
      "body": "\\setminus ",
      "description": "∖ (setminus)",
      "mode": "maths",
      "prefix": "\\\\\\\\\\\\$",
      "triggerWhenComplete": true
    },
    {
      "body": "\\begin{pmatrix} $1 \\end{pmatrix} ",
      "description": "pmatrix",
      "mode": "maths",
      "prefix": "\\bpmat$",
      "triggerWhenComplete": true
    },
    {
      "body": "\\begin{bmatrix} $1 \\end{bmatrix} ",
      "description": "bmatrix",
      "mode": "maths",
      "prefix": "\\bbmat$",
      "triggerWhenComplete": true
    },
    {
      "body": "\\frac{\\partial ${1:V}}{\\partial ${2:x}} ",
      "description": "partial derivative",
      "mode": "maths",
      "prefix": "\\bpart$",
      "triggerWhenComplete": true
    },
    {
      "body": "\\sqrt{$1}",
      "description": "√",
      "mode": "maths",
      "prefix": "\\bsq$",
      "triggerWhenComplete": true
    },
    {
      "body": "^2",
      "description": "²",
      "mode": "maths",
      "prefix": " ?sr$",
      "triggerWhenComplete": true
    },
    {
      "body": "^3",
      "description": "³",
      "mode": "maths",
      "prefix": " ?cb$",
      "triggerWhenComplete": true
    },
    {
      "body": "\\exists ",
      "description": "∃",
      "mode": "maths",
      "prefix": "\\bEE$",
      "triggerWhenComplete": true
    },
    {
      "body": "\\forall ",
      "description": "∀",
      "mode": "maths",
      "prefix": "\\bAA$",
      "triggerWhenComplete": true
    },
    {
      "body": "$1_$2",
      "description": "subscript letter",
      "mode": "maths",
      "prefix": "\\b([A-Za-z])([A-Za-z])\\2$",
      "triggerWhenComplete": true
    },
    {
      "body": "$1_{$2+1}",
      "description": "subscript letter + 1",
      "mode": "maths",
      "prefix": "\\b([A-Za-z])([A-Za-z])\\2?p1$",
      "priority": 2,
      "triggerWhenComplete": true
    },
    {
      "body": "\\int_{${1:-\\infty}}^{${2:\\infty}} ",
      "description": "∫ₐᵇ",
      "mode": "maths",
      "prefix": "\\bdint$",
      "triggerWhenComplete": true
    },
    {
      "body": "$1\\, ",
      "description": "add maths whitespace \\,",
      "mode": "maths",
      "prefix": "([^ \\\\])  $",
      "priority": -1,
      "triggerWhenComplete": true
    },
    {
      "body": "$1\\: ",
      "description": "add maths whitespace \\:",
      "mode": "maths",
      "prefix": "([^ \\\\])\\\\, {2,4}$",
      "priority": 0.1,
      "triggerWhenComplete": true
    },
    {
      "body": "$1\\; ",
      "description": "add maths whitespace \\;",
      "mode": "maths",
      "prefix": "([^ \\\\])\\\\: {2,4}$",
      "priority": 0.2,
      "triggerWhenComplete": true
    },
    {
      "body": "$1\\ ",
      "description": "add maths whitespace \\ ",
      "mode": "maths",
      "prefix": "([^ \\\\])\\\\; {2,4}$",
      "priority": 0.3,
      "triggerWhenComplete": true
    },
    {
      "body": "$1\\quad ",
      "description": "add maths whitespace quad",
      "mode": "maths",
      "prefix": "([^ \\\\])\\\\ {2,4}$",
      "priority": 0.4,
      "triggerWhenComplete": true
    },
    {
      "body": "$1\\qquad ",
      "description": "add maths whitespace qquad",
      "mode": "maths",
      "prefix": "([^ \\\\])\\\\quad {2,4}$",
      "priority": 0.5,
      "triggerWhenComplete": true
    },
    {
      "body": "\\\\{$1\\\\} ",
      "description": "set {}",
      "mode": "maths",
      "prefix": "\\bset$",
      "triggerWhenComplete": true
    },
    {
      "body": " \\mid ",
      "description": "∣",
      "mode": "maths",
      "prefix": " ?\\|\\|$",
      "triggerWhenComplete": true
    },
    {
      "body": "\\diamond ",
      "description": "⋄",
      "mode": "maths",
      "prefix": "\u003c ?\u003e$",
      "triggerWhenComplete": true
    },
    {
      "body": "\\begin{cases} $1 \\end{cases} ",
      "description": "cases",
      "mode": "maths",
      "prefix": "\\bcase$",
      "triggerWhenComplete": true
    },
    {
      "body": "$1\\text{s.t.} ",
      "description": "such that",
      "mode": "maths",
      "prefix": "(^|[^\\\\])\\bst$",
      "triggerWhenComplete": true
    },
    {
      "body": "\\pm ",
      "description": "±",
      "mode": "maths",
      "prefix": "\\+ ?-$",
      "priority": 1,
      "triggerWhenComplete": true
    },
    {
      "body": "\\mp ",
      "description": "∓",
      "mode": "maths",
      "prefix": "- ?\\+$",
      "priority": 1,
      "triggerWhenComplete": true
    },
    {
      "body": "\\binom{$1$2}{$3$4}",
      "description": "binomial",
      "mode": "maths",
      "prefix": "(?:([A-Za-z0-9]|\\\\\\w{,7})|\\(([^\\)]+)\\))C(?:([A-Za-z0-9]|\\\\\\w{,7})|\\(([^\\)]+)\\))$",
      "priority": 2,
      "triggerWhenComplete": true
    }
  ],

