  // Token colors (comments neutral but readable on true black)
  "editor.tokenColorCustomizations": {
    "comments": "#7E8591",
    "strings": "#A7C795",
    "numbers": "#F1D8A5",
    "keywords": "#8FB3D9",
    "functions": "#8ED1E3",
    "variables": "#E5E9F0",
    "types": "#94D0CC",
    "textMateRules": [
      {
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": { "foreground": "#7E8591", "fontStyle": "italic" }
      },
      {
        "scope": ["constant.numeric", "constant.language.boolean"],
        "settings": { "foreground": "#F1D8A5" }
      },
      {
        "scope": ["keyword", "storage.type", "storage.modifier"],
        "settings": { "foreground": "#8FB3D9" }
      },
      {
        "scope": ["entity.name.function", "support.function"],
        "settings": { "foreground": "#8ED1E3" }
      },
      {
        "scope": ["string", "string.quoted", "string.template"],
        "settings": { "foreground": "#A7C795" }
      },
      {
        "scope": ["entity.name.type", "support.type", "support.class"],
        "settings": { "foreground": "#94D0CC" }
      }
    ]
  },
  "editor.semanticTokenColorCustomizations": {
    "rules": {
      "comment": "#7E8591",
      "string": "#A7C795",
      "number": "#F1D8A5",
      "keyword": "#8FB3D9",
      "function": "#8ED1E3",
      "method": "#8ED1E3",
      "type": "#94D0CC",
      "property": "#E5E9F0",
      "variable": "#E5E9F0"
    }
  },

  // ===================================================================
  // EDITOR CORE / ERGONOMICS
  // ===================================================================
  "editor.detectIndentation": false,
  "editor.tabSize": 2,
  "editor.indentSize": "tabSize",
  "editor.insertSpaces": true,
  "editor.stickyTabStops": true,

  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 100,
  "editor.wrappingIndent": "indent",
  "editor.wrapOnEscapedLineFeeds": true,

  "editor.minimap.enabled": false,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.smoothScrolling": true,
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "all",
  "editor.bracketPairColorization.enabled": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.maxTokenizationLineLength": 200000,
  "editor.codeLensFontFamily": "Fira Code",
  "editor.allowVariableFontsInAccessibilityMode": true,

  "editor.fontFamily": "'Fira Code', 'Source Code Pro', 'Cascadia Code', Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontVariations": true,

  // Inline suggestions (Gemini preferred; avoid Copilot double-ghosting)
  "editor.inlineSuggest.enabled": true,
  "editor.inlineSuggest.showToolbar": "onHover",
  "editor.inlineSuggest.fontFamily": "'Fira Code'",
  "editor.suggest.preview": true,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.tabCompletion": "on",
  "editor.linkedEditing": true,
  "editor.wordBasedSuggestions": "matchingDocuments",
  "editor.aiStats.enabled": true,

  // Formatting defaults (Prettier) & triggers
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "modificationsIfAvailable",
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.codeActionsOnSave": {},

  // ===================================================================
  // FILES / SEARCH / WATCHERS
  // ===================================================================
  "files.eol": "\n",
  "files.trimTrailingWhitespace": true,
  "files.autoGuessEncoding": true,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 1000,

  "files.watcherExclude": {
    "**/.git/**": true,
    "**/node_modules/**": true,
    "**/out/**": true,
    "**/build/**": true,
    "**/.texlive202*/**": true
  },
  "search.exclude": {
    "**/.git/**": true,
    "**/node_modules/**": true,
    "**/out/**": true,
    "**/build/**": true,
    "**/.texlive202*/**": true
  },
  "search.followSymlinks": false,
  "search.useIgnoreFiles": true,

  // ===================================================================
  // LANGUAGE OVERRIDES
  // ===================================================================
  "[latex]": {
    "editor.defaultFormatter": "James-Yu.latex-workshop",
    "editor.formatOnSave": false,
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[bibtex]": { "editor.defaultFormatter": "James-Yu.latex-workshop" },
  "[json]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[jsonc]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },

  // ===================================================================
  // PRETTIER (GLOBAL)
  // ===================================================================
  "prettier.useTabs": false,
  "prettier.tabWidth": 2,
  "prettier.printWidth": 100,
  "prettier.proseWrap": "always",
  "prettier.singleAttributePerLine": true,
  "prettier.endOfLine": "lf",
  "prettier.vueIndentScriptAndStyle": true,
  "prettier.withNodeModules": true,

  // ===================================================================
  // LATEX WORKSHOP â€” BUILD/VIEW/LINT (matches your workspace recipes)
  // ===================================================================
  "workbench.editorAssociations": {
    "*.copilotmd": "vscode.markdown.preview.editor",
    "*.pdf": "latex-workshop-pdf-hook"
  },
  "latex-workshop.latex.autoBuild.run": "onFileChange",
  "latex-workshop.latex.autoBuild.interval": 5000,
  "latex-workshop.latex.build.forceRecipeUsage": true,
  "latex-workshop.latex.build.rootfileInStatus": true,
  "latex-workshop.latex.clean.subfolder.enabled": true,
  "latex-workshop.latex.autoClean.run": "never",
  "latex-workshop.latex.outDir": "out",
  "latex-workshop.latex.auxDir": "build",
  "latex-workshop.latex.recipe.default": "lastUsed",
  "latex-workshop.latex.recipes": [
    { "name": "BUILD-WIN", "tools": ["latexmk-win"] },
    { "name": "BUILD-WSL", "tools": ["latexmk-wsl"] },
    { "name": "AUTO", "tools": ["latexmk-pvc"] },
    { "name": "ON-THE-FLY", "tools": ["texliveonfly"] }
  ],
  "latex-workshop.latex.tools": [
    {
      "name": "latexmk-wsl",
      "command": "tools/latexmk-universal.sh",
      "args": [
        "-lualatex",
        "-outdir=out",
        "-auxdir=build",
        "-f",
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-shell-escape",
        "%DOC%"
      ]
    },
    {
      "name": "latexmk-win",
      "command": "latexmk",
      "args": [
        "-lualatex",
        "-outdir=out",
        "-auxdir=build",
        "-f",
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-shell-escape",
        "%DOC%"
      ]
    },
    {
      "name": "latexmk-pvc",
      "command": "latexmk",
      "args": [
        "-lualatex",
        "-outdir=out",
        "-auxdir=build",
        "-f",
        "-pvc",
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-shell-escape",
        "%DOC%"
      ]
    },
    {
      "name": "latexmk-auto",
      "command": "/usr/local/texlive/2025/bin/aarch64-linux/latexmk-auto",
      "args": ["%DOC%"]
    },
    {
      "name": "texliveonfly",
      "command": "/usr/local/texlive/2025/bin/aarch64-linux/texliveonfly",
      "args": ["%DOC%"]
    }
  ],
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.view.pdf.sidebar.open": "off",
  "latex-workshop.view.pdf.sidebar.view": "outline",
  "latex-workshop.view.pdf.reload.transition": "none",
  "latex-workshop.view.outline.sync.viewer": true,
  "latex-workshop.view.pdf.internal.synctex.keybinding": "ctrl-click",
  "latex-workshop.synctex.afterBuild.enabled": true,

  "latex-workshop.latex.rootFile.useSubFile": true,
  "latex-workshop.latex.rootFile.doNotPrompt": true,

  "latex-workshop.latex.watch.usePolling": true,
  "latex-workshop.latex.watch.pdf.delay": 100,
  "latex-workshop.linting.run": "onType",
  "latex-workshop.linting.chktex.convertOutput.column.chktexrcTabSize": 4,
  "latex-workshop.message.badbox.show": "none",
  "latex-workshop.message.information.show": true,
  "latex-workshop.message.log.show": true,

  // latexindent (exact args)
  "latex-workshop.formatting.latex": "latexindent",
  "latex-workshop.formatting.latexindent.path": "latexindent",
  "latex-workshop.formatting.latexindent.args": [
    "-w", // write-in-place
    "-s", // silent
    "-m", // only modify if changes occur
    "--overwriteIfDifferent" // avoids needless writes when unchanged
  ],

  // ===================================================================
  // LATEX UTILITIES / LATEX PREVIEW
  // ===================================================================
  "latex-utilities.formattedPaste.useAsDefault": true,
  "latex-utilities.formattedPaste.maxLineLength": 100,
  "latex-utilities.liveReformat.enabled": true,
  "latex-utilities.liveReformat.languages": ["tex", "doctex", "latex", "rsweave", "jlweave"],

  "latexPreview.latexFontDir": "C:\\\\Users\\\\vic-l\\\\AppData\\\\Local\\\\Programs\\\\MiKTeX\\\\fonts",
  "latexPreview.pageSize": "US Letter",

  // ===================================================================
  // MARKDOWN / NOTEBOOKS
  // ===================================================================
  "markdown.copyFiles.overwriteBehavior": "overwrite",
  "markdown.extension.completion.enabled": true,
  "markdown.extension.italic.indicator": "_",
  "markdown.extension.toc.orderedList": true,
  "markdown.preview.breaks": true,
  "markdown.preview.fontFamily": "'Fira Sans',-apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', system-ui, 'Ubuntu', 'Droid Sans', sans-serif",
  "markdown.preview.lineHeight": 1.2,

  "markdown-preview-enhanced.automaticallyShowPreviewOfMarkdownBeingEdited": true,
  "markdown-preview-enhanced.enableCriticMarkupSyntax": true,
  "markdown-preview-enhanced.enableExtendedTableSyntax": true,
  "markdown-preview-enhanced.enableHTML5Embed": true,
  "markdown-preview-enhanced.enableTypographer": true,
  "markdown-preview-enhanced.latexEngine": "lualatex",
  "markdownPreviewEnhanced.previewTheme": "one-dark",

