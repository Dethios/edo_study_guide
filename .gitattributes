*.pdf filter=lfs diff=lfs merge=lfs -text
*.zip filter=lfs diff=lfs merge=lfs -text

# Text defaults
* text=auto eol=lf

# Windows scripts that should stay CRLF
*.bat text eol=crlf
*.cmd text eol=crlf
*.ps1 text eol=crlf

# Binary
*.png binary
*.jpg binary
*.pdf binary

# Build artifacts
out/**  -text
build/** -text
